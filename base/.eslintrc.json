{
  "rules": {
    "strict": [2, "global"],
    "comma-style": 2,
    "comma-spacing": 2,
    "indent": [2, 2, { "SwitchCase": 1, "outerIIFEBody": 1, "MemberExpression": 0 }],
    "quotes": [2, "single"],
    "linebreak-style": 2,
    "semi": [2, "always"],
    "semi-spacing": [2, { "before": false, "after": true }],
    "max-len": [2, 120, 2, { "ignoreComments": true } ],
    "dot-location": [2, "property"],
    "no-duplicate-imports": [2, { "includeExports": true }],

    "eqeqeq": 2,
    "block-scoped-var": 2,
    "no-constant-condition": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-lonely-if": 2,
    "no-lone-blocks": 2,
    "no-nested-ternary": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-dupe-keys": 2,
    "no-extra-boolean-cast": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-else-return": 0,
    "no-eval": 2,
    "no-multi-str": 2,
    "no-self-compare": 2,
    "no-useless-call": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-extra-semi": 2,

    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "space-infix-ops": 2,
    "space-unary-ops": 2,

    "radix": 2,
    "curly": 2,
    "no-fallthrough": 0,
    "default-case": 0,
    "no-plusplus": 0,
    "dot-notation": 0,
    "eol-last": 2
  }
}
