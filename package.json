{
  "name": "eslint-config-dabapps",
  "version": "5.0.0",
  "description": "DabApps ESLint Configuration",
  "main": ".eslintrc.json",
  "scripts": {
    "jsonlint": "find . -path ./node_modules -prune -o -type f -name '*.json' -exec jsonlint -q '{}' \\;",
    "lint": "eslint -c .eslintrc.json '*.{ts,tsx,js}'",
    "test": "npm run jsonlint && npm run eslint",
    "prettier": "prettier --write '*.{ts,tsx,js}'",
    "prettier-check": "prettier --check '*.{ts,tsx,js}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabapps/eslint-config-dabapps"
  },
  "keywords": [
    "eslint",
    "linting",
    "react",
    "react-native",
    "es6",
    "javascript",
    "typescript",
    "config",
    "configuration",
    "dabapps"
  ],
  "author": "DabApps",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/dabapps/eslint-config-dabapps/issues"
  },
  "homepage": "https://github.com/dabapps/eslint-config-dabapps",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-eslint": "^10.0.3",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jsonlint": "^1.6.3",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "jquery": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
