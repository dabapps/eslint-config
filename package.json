{
  "name": "eslint-config-dabapps",
  "version": "3.0.0",
  "description": "DabApps ESLint Configuration",
  "main": ".eslintrc.json",
  "scripts": {
    "jsonlint": "find . -path ./node_modules -prune -o -type f -name '*.json' -exec jsonlint -q '{}' \\;",
    "eslint": "eslint -c .eslintrc.json test-pass.js",
    "test": "npm run jsonlint && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabapps/eslint-config"
  },
  "keywords": [
    "eslint",
    "linting",
    "react",
    "react-native",
    "es6",
    "javascript",
    "config",
    "configuration",
    "dabapps"
  ],
  "author": "DabApps",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/dabapps/eslint-config/issues"
  },
  "homepage": "https://github.com/dabapps/eslint-config",
  "dependencies": {
    "babel-eslint": "7.1.0",
    "eslint-plugin-react": "=6.7.1",
    "eslint-plugin-react-native": "=2.0.0"
  },
  "devDependencies": {
    "jsonlint": "=1.6.2"
  },
  "peerDependencies": {
    "eslint": "=3.10.0",
  }
}
