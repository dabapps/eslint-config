{
  "name": "eslint-config-dabapps",
  "version": "5.0.0",
  "description": "DabApps ESLint Configuration",
  "main": ".eslintrc.json",
  "scripts": {
    "jsonlint": "find . -path ./node_modules -prune -o -type f -name '*.json' -exec jsonlint -q '{}' \\;",
    "eslint": "eslint -c .eslintrc.json test-pass.js",
    "test": "npm run jsonlint && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabapps/eslint-config-dabapps"
  },
  "keywords": [
    "eslint",
    "linting",
    "react",
    "react-native",
    "es6",
    "javascript",
    "config",
    "configuration",
    "dabapps"
  ],
  "author": "DabApps",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/dabapps/eslint-config-dabapps/issues"
  },
  "homepage": "https://github.com/dabapps/eslint-config-dabapps",
  "dependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "jsonlint": "^1.6.3",
    "react": "^16.12.0"
  },
  "optionalDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-react-hooks": "^2.3.0"
  }
}
