{
  "name": "eslint-config-dabapps",
  "version": "5.0.0",
  "description": "DabApps ESLint Configuration",
  "main": ".eslintrc.json",
  "scripts": {
    "jsonlint": "find . -path ./node_modules -prune -o -type f -name '*.json' -exec jsonlint -q '{}' \\;",
    "eslint": "eslint -c .eslintrc.json test-pass.js",
    "test": "npm run jsonlint && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabapps/eslint-config-dabapps"
  },
  "keywords": [
    "eslint",
    "linting",
    "react",
    "react-native",
    "es6",
    "javascript",
    "config",
    "configuration",
    "dabapps"
  ],
  "author": "DabApps",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/dabapps/eslint-config-dabapps/issues"
  },
  "homepage": "https://github.com/dabapps/eslint-config-dabapps",
  "dependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "jsonlint": "^1.6.3",
    "react": "^16.12.0"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "prettier": "1.19.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1"
  },
  "optionalDependencies": {
    "babel-eslint": "10.0.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-native": "3.8.1"
  }
}
