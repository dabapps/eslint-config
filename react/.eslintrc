{
  "extends": "../base/.eslintrc",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx":true
    }
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "jsx-quotes": [2, "prefer-double"],

    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  }
}
